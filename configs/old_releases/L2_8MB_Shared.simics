flexus.set "-L2:dir_config" "total_sets=256:assoc=16:skew=false"
flexus.set "-L2:dir_eb_size" "16"
flexus.set "-L2:dir_issue_lat" "1"
flexus.set "-L2:dir_lat" "3"
flexus.set "-L2:dir_type" "std"
flexus.set "-L2:group_interleaving" "1024"
flexus.set "-L2:groups" "1"
flexus.set "-L2:maf_size" "32"
flexus.set "-L2:policy" "NonInclusiveMESI"
flexus.set "-L2:queue_size" "8"
