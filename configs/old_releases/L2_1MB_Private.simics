flexus.set "-L2:evict_on_snoop" "0"
flexus.set "-L2:fast_evict_clean" "0"
flexus.set "-L2:maf" "40"
flexus.set "-L2:maf_targets" "0"
flexus.set "-L2:no_bus" "0"
flexus.set "-L2:ports" "1"
flexus.set "-L2:pre_queue_size" "4"
flexus.set "-L2:probe_fetchmiss" "1"
