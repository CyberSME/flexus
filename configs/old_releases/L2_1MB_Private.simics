# L2 Trace
flexus.set "-L2:assoc" "16"
flexus.set "-L2:block_scout" "0"
flexus.set "-L2:bsize" "64"
flexus.set "-L2:clean_evict" "1"
flexus.set "-L2:erb_size" "8"
flexus.set "-L2:mt_width" "1"
flexus.set "-L2:notify_reads" "0"
flexus.set "-L2:notify_writes" "0"
flexus.set "-L2:protocol" "InclusiveMOESI"
flexus.set "-L2:rsize" "1024"
flexus.set "-L2:rt_assoc" "16"
flexus.set "-L2:rt_repl" "LRU"
flexus.set "-L2:rt_size" "8192"
