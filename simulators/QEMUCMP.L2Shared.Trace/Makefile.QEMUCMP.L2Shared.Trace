DEFAULT_TARGET = qemu
SUPPORTS_X86 = true
SUPPORTS_V9 = true
DEFAULT_PLATFORM = x86

SIMICS_CLASSES = CLS:Flexus;CLS:QemuInterface;CLS:DecoupledFeederQEMU
